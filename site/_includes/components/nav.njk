<nav id="nav" class="nav">
    <ul class="nav-list">
        {% for item in navigation.main %}
            {% if item.cta %}
                <li class="nav-list-item">
                    <a href="{{ item.url | safe }}" class="cta {% if page.url == item.url %}active{% endif %}" {% if page.url == item.url %} aria-current="page"{% endif %}>{{ item.label }}</a>
                </li>
            {% else %}
                <li class="nav-list-item">
                    <a href="{{ item.url | safe }}" class="{% if page.url == item.url %}active{% endif %}" {% if page.url == item.url %} aria-current="page"{% endif %}>{{ item.label }}</a>
                </li>
            {% endif %}
        {% endfor %}
    </ul>
</nav>