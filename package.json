{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "scss": "node-sass assets/scss -o assets/css",
    "css": "postcss assets/css/*.css -u autoprefixer cssnano -r -m",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "node-sass --watch assets/scss -o assets/css",
    "develop": "npm run scss ; ELEVENTY_ENV=dev npm run watch:eleventy & npm run watch:sass",
    "build": "npm run scss ; ELEVENTY_ENV=prod eleventy ; npm run css"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "autoprefixer": "^9.8.5",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.0.0",
    "include-media": "^1.4.9",
    "luxon": "^1.21.3",
    "node-sass": "^4.11.0",
    "postcss-cli": "^7.1.0",
    "postcss-normalize": "^9.0.0"
  },
  "dependencies": {
    "eleventy-plugin-sass": "^1.0.0",
    "html-minifier": "^4.0.0"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
